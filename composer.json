{
    "name": "other-solution/klix-app-merchant-api-php",
    "description": "OtherSolution: Klix merchant API PHP SDK",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "klix",
        "api",
        "sdk",
        "merchant",
        "other-solution",
        "php",
        "php8"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "phpsed",
            "role": "Owner",
            "homepage": "https://github.com/phpsed"
        },
        {
            "name": "sigulda",
            "role": "Developer",
            "homepage": "https://github.com/sigulda"
        }
    ],
    "support": {
        "issues": "https://github.com/other-solution/klix-app-merchant-api-php/issues"
    },
    "require": {
        "php": ">=8.0.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "doctrine/collections": "*",
        "jetbrains/phpstorm-attributes": "*",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/http-client-contracts": "*",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0"
    },
    "require-dev": {
        "doctrine/annotations": "^1.13",
        "friendsofphp/php-cs-fixer": ">=v3.8.0",
        "phpunit/phpunit": ">=9.5.0",
        "roave/security-advisories": "dev-latest",
        "symfony/cache": "^5.4",
        "symfony/serializer": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
            "OtherSolution\\Klix\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OtherSolution\\Klix\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "thanks": {
            "name": "other-solution/klix-app-merchant-api-php",
            "url": "https://github.com/other-solution/klix-app-merchant-api-php"
        }
    }
}
