{
    "name": "other-solution/klix-app-merchant-api-php",
    "description": "OtherSolution: Klix merchant API PHP SDK",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "klix",
        "api",
        "sdk",
        "merchant",
        "other-solution",
        "php",
        "php8"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "phpsed",
            "role": "Owner",
            "homepage": "https://github.com/phpsed"
        },
        {
            "name": "sigulda",
            "role": "Developer",
            "homepage": "https://github.com/sigulda"
        }
    ],
    "support": {
        "issues": "https://github.com/other-solution/klix-app-merchant-api-php/issues"
    },
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-openssl": "*",
        "jetbrains/phpstorm-attributes": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.5.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "OtherSolution\\Klix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OtherSolution\\Klix\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "thanks": {
            "name": "other-solution/klix-app-merchant-api-php",
            "url": "https://github.com/other-solution/klix-app-merchant-api-php"
        }
    }
}
